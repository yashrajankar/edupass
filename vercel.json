{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "config.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...auth].js"
    },
    {
      "src": "/admin-dashboard.html",
      "dest": "/admin-dashboard.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/dashboard.html"
    },
    {
      "src": "/exams.html",
      "dest": "/exams.html"
    },
    {
      "src": "/notifications.html",
      "dest": "/notifications.html"
    },
    {
      "src": "/profile.html",
      "dest": "/profile.html"
    },
    {
      "src": "/room.html",
      "dest": "/room.html"
    },
    {
      "src": "/seating.html",
      "dest": "/seating.html"
    },
    {
      "src": "/staff.html",
      "dest": "/staff.html"
    },
    {
      "src": "/students.html",
      "dest": "/students.html"
    },
    {
      "src": "/timetable.html",
      "dest": "/timetable.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}